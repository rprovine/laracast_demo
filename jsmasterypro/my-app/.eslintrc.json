{
"env": {
    "browser": true,
    "es2021": true,
    "node": true
},
"extends": [
    "standard"
],
"parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
},
"rules": {
    "no-console": "warn",
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "comma-dangle": ["error", "never"]
}
}

